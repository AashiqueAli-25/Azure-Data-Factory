{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfpractice11nov"
		},
		"LS_EXCELFILE_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_EXCELFILE'"
		},
		"LS_PRQ_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_PRQ'"
		},
		"LS_EXCELFILE_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsbulkload11nov.dfs.core.windows.net/"
		},
		"LS_PRQ_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlssinknov11.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfpractice11nov_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_EXCELFILE_accountKey": {
						"value": "[parameters('LS_EXCELFILE_accountKey')]"
					},
					"LS_PRQ_accountKey": {
						"value": "[parameters('LS_PRQ_accountKey')]"
					},
					"LS_EXCELFILE_properties_typeProperties_url": {
						"value": "[parameters('LS_EXCELFILE_properties_typeProperties_url')]"
					},
					"LS_PRQ_properties_typeProperties_url": {
						"value": "[parameters('LS_PRQ_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}